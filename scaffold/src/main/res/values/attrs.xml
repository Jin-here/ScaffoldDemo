<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AVLoadingIndicatorView">
        <attr name="indicator">
            <flag name="BallPulse" value="0"/>
            <flag name="BallGridPulse" value="1"/>
            <flag name="BallClipRotate" value="2"/>
            <flag name="BallClipRotatePulse" value="3"/>
            <flag name="SquareSpin" value="4"/>
            <flag name="BallClipRotateMultiple" value="5"/>
            <flag name="BallPulseRise" value="6"/>
            <flag name="BallRotate" value="7"/>
            <flag name="CubeTransition" value="8"/>
            <flag name="BallZigZag" value="9"/>
            <flag name="BallZigZagDeflect" value="10"/>
            <flag name="BallTrianglePath" value="11"/>
            <flag name="BallScale" value="12"/>
            <flag name="LineScale" value="13"/>
            <flag name="LineScaleParty" value="14"/>
            <flag name="BallScaleMultiple" value="15"/>
            <flag name="BallPulseSync" value="16"/>
            <flag name="BallBeat" value="17"/>
            <flag name="LineScalePulseOut" value="18"/>
            <flag name="LineScalePulseOutRapid" value="19"/>
            <flag name="BallScaleRipple" value="20"/>
            <flag name="BallScaleRippleMultiple" value="21"/>
            <flag name="BallSpinFadeLoader" value="22"/>
            <flag name="LineSpinFadeLoader" value="23"/>
            <flag name="TriangleSkewSpin" value="24"/>
            <flag name="Pacman" value="25"/>
            <flag name="BallGridBeat" value="26"/>
            <flag name="SemiCircleSpin" value="27"/>
        </attr>
        <attr name="indicator_color" format="color"/>
    </declare-styleable>

    <declare-styleable name="UltraViewPager">
        <attr name="upv_autoscroll" format="integer" />
        <attr name="upv_infiniteloop" format="boolean" />
        <attr name="upv_ratio" format="float" />
        <attr name="upv_scrollmode" format="enum">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
        <attr name="upv_disablescroll" format="enum">
            <enum name="none" value="0" />
            <enum name="backward" value="1" />
            <enum name="forward" value="2" />
        </attr>
        <attr name="upv_multiscreen" format="float" />
        <attr name="upv_automeasure" format="boolean" />
        <attr name="upv_itemratio" format="float" />
    </declare-styleable>

    <declare-styleable name="WaveView">
        <attr name="waveMinRadius" format="dimension"/>
        <attr name="waveMaxRadius" format="dimension"/>
        <attr name="waveColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="MaskView">
        <attr name="mask" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="BottomNavigationItem">
        <attr name="bottomNavIcon" format="reference"/>
        <attr name="bottomNavIconChecked" format="reference"/>
        <attr name="bottomNavName" format="string"/>
    </declare-styleable>

    <declare-styleable name="RoundRectImageView">
        <attr name="roundRectStrokeWidth" format="dimension"/>
        <attr name="roundRectStrokeColor" format="color"/>
        <attr name="roundRectRadius" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="TitleLayout">
        <attr name="titleCaption" format="string"/>
        <attr name="titleBackIcon" format="reference"/>
        <attr name="titleMenu" format="string"/>
        <attr name="titleMenuEnabled" format="boolean"/>
        <attr name="titleShowShadow" format="boolean"/>
        <attr name="titleDarkMode" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="SlidingTabLayout">
        <attr name="tabEqualization" format="boolean"/>
        <attr name="tabPadding" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="SettingItemView">
        <attr name="settingName" format="string"/>
        <attr name="settingIcon" format="reference"/>
        <attr name="settingDes" format="string"/>
    </declare-styleable>

    <declare-styleable name="SettingCheckView">
        <attr name="settingCheckName" format="string"/>
        <attr name="settingCheckIcon" format="reference"/>
        <attr name="settingCheckDes" format="string"/>
        <attr name="settingChecked" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="SettingToggleView">
        <attr name="settingToggleName" format="string"/>
        <attr name="settingToggleIcon" format="reference"/>
        <attr name="settingToggleDes" format="string"/>
        <attr name="settingToggled" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="ImgTxtLayout">
        <attr name="imgTxtIcon" format="reference"/>
        <attr name="imgTxtIconBackground" format="reference"/>
        <attr name="imgTxtIconSize" format="dimension"/>
        <attr name="imgTxtName" format="string"/>
        <attr name="imgTxtNameColor" format="color"/>
        <attr name="imgTxtNameSize" format="dimension"/>
        <attr name="imgTxtPadding" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="TxtTxtLayout">
        <attr name="txtUp" format="reference"/>
        <attr name="txtBelow" format="reference"/>
        <attr name="txtUpSize" format="dimension"/>
        <attr name="txtBelowSize" format="dimension"/>
        <attr name="txtUpColor" format="color"/>
        <attr name="txtBelowColor" format="color"/>
        <attr name="txtPadding" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="CountEditText">
        <attr name="countMaxLength" format="integer"/>
        <attr name="countHint" format="string"/>
    </declare-styleable>
</resources>