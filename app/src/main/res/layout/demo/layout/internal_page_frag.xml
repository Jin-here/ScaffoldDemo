<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        android:id="@+id/internalPageScan"
        android:layout_width="match_parent"
        android:layout_height="64dp"
        style="@style/Body1.Black.High"
        android:gravity="center_vertical|start"
        android:paddingStart="10dp"
        android:text="二维码识别"/>

    <TextView
        android:id="@+id/internalPageChooseImg"
        android:layout_width="match_parent"
        android:layout_height="64dp"
        style="@style/Body1.Black.High"
        android:gravity="center_vertical|start"
        android:paddingStart="10dp"
        android:text="选择相册/拍照"/>

    <TextView
        android:id="@+id/internalPagePermission"
        android:layout_width="match_parent"
        android:layout_height="64dp"
        style="@style/Body1.Black.High"
        android:gravity="center_vertical|start"
        android:paddingStart="10dp"
        android:text="权限申请"/>

    <TextView
        android:id="@+id/internalPageCheckVersion"
        android:layout_width="match_parent"
        android:layout_height="64dp"
        style="@style/Body1.Black.High"
        android:gravity="center_vertical|start"
        android:paddingStart="10dp"
        android:text="检查更新"/>

    <TextView
        android:id="@+id/internalPageFeedback"
        android:layout_width="match_parent"
        android:layout_height="64dp"
        style="@style/Body1.Black.High"
        android:gravity="center_vertical|start"
        android:paddingStart="10dp"
        android:text="用户反馈"/>

    <TextView
        android:id="@+id/internalPageImgPreview"
        android:layout_width="match_parent"
        android:layout_height="64dp"
        style="@style/Body1.Black.High"
        android:gravity="center_vertical|start"
        android:paddingStart="10dp"
        android:text="图片预览"/>
</LinearLayout>